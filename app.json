{
	"expo": {
		"name": "Liosta - Simple Offline Lists",
		"slug": "liosta",
		"scheme": "liosta",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#212121"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon.png",
				"backgroundColor": "#212121"
			}
		},
		"web": {
			"favicon": "./assets/icon.png",
			"bundler": "metro"
		},
		"plugins": [
			"expo-router"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}